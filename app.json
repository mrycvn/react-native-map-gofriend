{
  "expo": {
    "name": "GOFRIEND",
    "slug": "GOFRIEND",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location"
        ]
      }
    },
    "android": {
      "package": "com.mrycvn.gofriend"
    }
  },
  "android": {
    "packagename": "com.mrycvn.gofriend",
    "adaptiveIcon": {
      "foregroundImage": "./assets/adaptive-icon.png",
      "backgroundColor": "#FFFFFF"
    },
    "useNextNotificationsApi": true,
    "permissions": [
      "LOCATION",
      "LOCATION_FOREGROUND",
      "ACCESS_COARSE_LOCATION",
      "ACCESS_FINE_LOCATION",
      "ACCESS_BACKGROUND_LOCATION"
    ]
  },
  "web": {
    "favicon": "./assets/favicon.png"
  }
}
